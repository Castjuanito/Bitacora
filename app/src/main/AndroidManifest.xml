<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bitacora">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/escudo_universidad_salle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RecuerdosActivity"></activity>
        <activity android:name=".PaisajesActivity" />
        <activity android:name=".MuestrasActivity" />
        <activity android:name=".MisViajesActivity" />
        <activity android:name=".InfraestructurasActivity" />
        <activity android:name=".DetalleViajeActivity" />
        <activity android:name=".CrearViajeActivity" />
        <activity android:name=".CrearRecuerdoActivity" />
        <activity android:name=".CrearPaisajeActivity" />
        <activity android:name=".CrearMuestrasActivity" />
        <activity android:name=".CrearInfraestructuraActivity" />
        <activity android:name=".AlistamientoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>